{
  "private": true,
  "name": "app-exports",
  "description": "A Commerce Layer Single Page Application to export resources with a nice GUI.",
  "version": "2.0.0",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "reactjs",
    "jamstack",
    "headless",
    "ecommerce",
    "api",
    "imports",
    "commercelayer"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
