    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "7.51.5",
    "wouter": "^3.2.0"
