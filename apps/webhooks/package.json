{
  "private": true,
  "name": "app-webhooks",
  "description": "A Commerce Layer Single Page Application to manage webhooks with a nice GUI.",
  "version": "2.0.0",
  "license": "MIT",
  "keywords": [
    "reactjs",
    "jamstack",
    "headless",
    "ecommerce",
    "api",
    "webhooks",
    "commercelayer"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "ts:check": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "prepare": "touch ./public/config.local.js"
  },
  "dependencies": {
    "@commercelayer/app-elements": "1.24.0",
    "@commercelayer/sdk": "5.37.0",
    "@hookform/resolvers": "^3.3.4",
    "lodash": "^4.17.21",
    "phosphor-react": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "swr": "^2.2.5",
    "type-fest": "^4.18.1",
    "wouter": "^3.1.2",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@commercelayer/app-builder": "workspace:*",
    "@commercelayer/eslint-config-ts-react": "^1.4.5",
    "@types/lodash": "^4.17.0",
    "@types/node": "20.12.8",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "rollup-plugin-external-globals": "^0.10.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.3"
  },
  "engines": {
    "node": ">=18"
  }
}
